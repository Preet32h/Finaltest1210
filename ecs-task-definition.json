{
  "family": "singh-api-task-definition",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "singh-api-container",
      "image": "536697260132.dkr.ecr.us-east-1.amazonaws.com/singh-final-api:latest",
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
